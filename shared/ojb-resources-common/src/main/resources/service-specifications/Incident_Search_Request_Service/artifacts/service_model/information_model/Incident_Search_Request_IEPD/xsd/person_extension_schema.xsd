<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ojbc.org/IEPD/Extensions/IncidentPersonSearchRequest/1.0"
	elementFormDefault="qualified" xmlns:s="http://niem.gov/niem/structures/2.0"
	xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:inc="http://ojbc.org/IEPD/Extensions/IncidentSearchRequest/1.0">
	<xsd:import namespace="http://niem.gov/niem/niem-core/2.0"
		schemaLocation="Subset/niem/niem-core/2.0/niem-core.xsd" />
	<xsd:import namespace="http://niem.gov/niem/structures/2.0"
		schemaLocation="Subset/niem/structures/2.0/structures.xsd" />
	<xsd:import namespace="http://ojbc.org/IEPD/Extensions/IncidentSearchRequest/1.0"
		schemaLocation="incident_extension_schema.xsd" />

	<xsd:complexType name="IncidentPersonSearchRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for an incident search request using
				Person criteria.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Person" />
					<xsd:element ref="inc:SourceSystemNameText" minOccurs="0"
						maxOccurs="unbounded" />
						<xsd:element ref="inc:SearchMetadata" minOccurs="0"
						maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>