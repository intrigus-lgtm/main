<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:doc="http://ojbc.org/IEPD/Exchange/JuvenileOffenseHistoryResponse/1.0" xmlns:cext="http://ojbc.org/IEPD/Extension/JuvenileHistory/JuvenileHistoryCommonExtension/1.0" xmlns:ext="http://ojbc.org/IEPD/Extension/JuvenileOffenseHistoryExtension/1.0" xmlns:structures="http://release.niem.gov/niem/structures/3.0/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:cyfs="http://release.niem.gov/niem/domains/cyfs/3.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/5.0/" xmlns:appinfo="http://release.niem.gov/niem/appinfo/3.0/" targetNamespace="http://ojbc.org/IEPD/Exchange/JuvenileOffenseHistoryResponse/1.0" version="1">
	<xsd:import namespace="http://release.niem.gov/niem/structures/3.0/" schemaLocation="../Subset/niem/structures/3.0/structures.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/5.0/" schemaLocation="../Subset/niem/domains/jxdm/5.0/jxdm.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/domains/cyfs/3.0/" schemaLocation="../Subset/niem/domains/cyfs/3.0/cyfs.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/3.0/" schemaLocation="../Subset/niem/niem-core/3.0/niem-core.xsd"/>
	<xsd:import namespace="http://ojbc.org/IEPD/Extension/JuvenileOffenseHistoryExtension/1.0" schemaLocation="JuvenileOffenseHistoryExtension.xsd"/>
	<xsd:import namespace="http://ojbc.org/IEPD/Extension/JuvenileHistory/JuvenileHistoryCommonExtension/1.0" schemaLocation="../JuvenileHistoryCommonExtension.xsd"/>
	<xsd:complexType name="JuvenileOffenseHistoryResponseType">
		<xsd:annotation>
			<xsd:documentation>A data type for a response to a query for demographic information related to a juvenile.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:DocumentType">
				<xsd:sequence>
				<xsd:element ref="cext:JuvenileHistoryCategoryCode"/>
					<xsd:element ref="cext:JuvenileHistoryQueryCriteria"/>
					<xsd:element ref="ext:JuvenileOffenseHistory"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="JuvenileOffenseHistoryResponse" type="doc:JuvenileOffenseHistoryResponseType">
		<xsd:annotation>
			<xsd:documentation>A response to a query for the offense history of a juvenile.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
