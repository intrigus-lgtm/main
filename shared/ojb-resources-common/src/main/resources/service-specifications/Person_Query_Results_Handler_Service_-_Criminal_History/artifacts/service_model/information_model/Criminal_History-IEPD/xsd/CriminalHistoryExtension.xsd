<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema 
	xmlns:Q1="http://niem.gov/niem/structures/2.0" 
	xmlns:Q2="http://ojbc.org/IEPD/Extensions/RapSheet/1.0" 
	xmlns:Q3="http://ojbc.org/IEPD/Extensions/PersonQueryErrorReporting/1.0" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://ojbc.org/IEPD/Extensions/CriminalHistory/1.0">
